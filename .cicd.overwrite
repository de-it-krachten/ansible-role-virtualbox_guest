---

# Generic
generic:
  role: ansible-role-virtualbox_guest
  role_short: virtualbox_guest
  role_description: |
    Compiles Virtualbox guest additions from source

github:
  fail_fast: false

platforms_add:
  - name: Fedora 35
    image: fedora35
  - name: Fedora 36
    image: fedora36

molecule:
  active: false
  platforms_add:
    - name: fedora35
      image: "geerlingguy/docker-fedora35-ansible:latest"
    - name: fedora36
      image: "geerlingguy/docker-fedora36-ansible:latest"
